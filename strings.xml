    <!-- Introduction Summaries -->
    <string name="is_wiki">Use the @wiki command to fetch the summary of any Wikipedia article \n\nExample: Paris @wiki</string>
    <string name="is_calculator">Use the @calc command to perform calculations right inside the textbox. Supports addition, subtraction, muliplication, division and parentheses \n\nExample: (96/8) - 9*5 @calc</string>
    <string name="is_news">Use @news to fetch the news regarding any subject \n\nExample: Android @news</string>
    <string name="is_imdb">Use @imdb to fetch movie ratings. Also supports Rotten Tomatoes \n\nExample: Inception @imdb</string>
    <string name="is_giphy">Use @gif to fecth gifs from Giphy \n\nExample: Cats @gif</string>
    <string name="is_bing">Use @pic to fetch images from Bing \n\nExample: Emma Watson @pic</string>
    <string name="is_weather">Use @weather to fetch current day\'s weather and @forecast for 5-day forecast of any city \n\nExample: Miami @weather, Miami @forecast</string>
    <string name="is_horoscope">Use @horoscope to fetch daily horoscope \n\nExample: Sagittarius @horoscope</string>

