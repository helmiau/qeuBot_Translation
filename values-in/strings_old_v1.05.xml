    <!-- Introduction Summaries -->
	<string name="is_wiki">"Gunakan perintah @wiki untuk mengambil ringkasan dari setiap artikel Wikipedia \n\nContoh: Paris @wiki"</string>
    <string name="is_calculator">"Gunakan perintah @calc untuk melakukan perhitungan tepat di dalam kotak teks. Mendukung penambahan, pengurangan, perkalian, pembagian, dan tanda kurung \n\nContoh: (96/8) - 9 * 5 @calc"</string>
    <string name="is_news">"Gunakan @news untuk mengambil berita tentang subjek apapun \n\nContoh: Android @news"</string>
    <string name="is_imdb">"Gunakan @imdb untuk mengambil peringkat film. Juga mendukung Rotten Tomatoes \n\nContoh: Inception @imdb"</string>
    <string name="is_giphy">"Gunakan @gif untuk mengambil gifs dari Giphy \n\nContoh: @gif Cats"</string>
    <string name="is_bing">"Gunakan @pic untuk mengambil gambar dari Bing \n\nContoh: Emma Watson @pic"</string>
    <string name="is_weather">"Gunakan @weather untuk mengambil cuaca hari ini dan @forecast untuk perkiraan 5-hari setiap kota \n\nContoh: Miami @weather, Miami @forecast"</string>
    <string name="is_horoscope">"Gunakan @horoscope untuk mengambil ramalan bintang harian \n\nContoh: Sagitarius @horoscope"</string>

